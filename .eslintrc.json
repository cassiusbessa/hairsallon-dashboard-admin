{
    "env": {
        "es2021": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "airbnb",
        "airbnb/hooks"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "off",
        "react/jsx-curly-brace-presence": "off",
        "object-curly-newline": "off",
        "dot-notation": "off",
        "react/prop-types": "off",
        "max-len": 0
    }
}
